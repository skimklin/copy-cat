{
  "name": "copy-cat",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.ts",
  "jsnext:main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "sideEffects": false,
  "srctype": "ts",
  "scripts": {
    "rename": "node rename.js",
    "clean": "rimraf ./dist",
    "lint": "eslint -c .eslintrc.js 'src/*'",
    "lint:fix": "eslint -c .eslintrc.js 'src/**/*.ts' --fix",
    "build:self": "rollup -c config/rollup.config.js",
    "build:esm": "rollup -c config/rollup.config.esm.js",
    "build:aio": "rollup -c config/rollup.config.aio.js",
    "build:aio.min": "cross-env NODE_ENV=production npm run build:aio",
    "build": "npm run clean && npm run build:self && npm run build:esm && npm run build:aio && npm run build:aio.min",
    "coveralls": "nyc --nycrc-path .nycrc-ts report --reporter=text-lcov | coveralls",
    "release": "npm test && npm run build && git commit -am $npm_package_version && git push && git tag $npm_package_version && git push --tags",
    "test": "jest --verbose"
  },
  "jest": {
    "rootDir": "tests",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "coffee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/skimklin/copy-cat.git"
  },
  "bugs": {
    "url": "https://github.com/skimklin/copy-cat/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "7.0.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.3",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-plugin-istanbul": "5.1.0",
    "cdkit": "1.1.0",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "es5-shim": "4.5.10",
    "eslint": "^5.15.1",
    "expect.js": "0.3.1",
    "jest": "^24.8.0",
    "nyc": "13.1.0",
    "rimraf": "2.6.2",
    "rollup": "0.57.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "8.3.0",
    "rollup-plugin-node-resolve": "3.0.3",
    "rollup-plugin-typescript2": "^0.22.0",
    "rollup-plugin-uglify": "1.0.0",
    "source-map-support": "0.5.9",
    "ts-jest": "^24.0.2",
    "ts-node": "^7.0.1",
    "tslib": "1.9.3",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "@jsmini/extend": "0.2.1",
    "@jsmini/type": "0.4.0"
  }
}
